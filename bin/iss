#!/usr/bin/env python3
# -*- coding: utf-8 -*-
import re
import sys
import os

sys.path.append('{}/../extlibs'.format('/'.join(os.path.abspath(__file__).split('/')[:-1])))
#print(sys.path)

from iss.app import main

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(main())
